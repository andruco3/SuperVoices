<% if @administrator.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      El formulario contuene <%= pluralize(@administrator.errors.count, "errores") %>.
    </div>
    <ul>
      <% @administrator.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>